{
  "name": "openclaw-mobile",
  "version": "2.0.0",
  "description": "Ultimate Free AI Assistant - 100% Gratis, Phone Optimized, Autonomous Agent",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:sandbox": "wrangler pages dev dist --ip 0.0.0.0 --port 3000",
    "build": "vite build",
    "preview": "wrangler pages dev dist",
    "deploy": "npm run build && wrangler pages deploy dist",
    "deploy:prod": "npm run build && wrangler pages deploy dist --project-name openclaw-mobile",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "clean-port": "fuser -k 3000/tcp 2>/dev/null || true",
    "test": "curl http://localhost:3000/api/health",
    "git:init": "git init && git add . && git commit -m 'Initial commit'",
    "git:commit": "git add . && git commit -m",
    "git:status": "git status",
    "git:log": "git log --oneline"
  },
  "dependencies": {
    "@huggingface/inference": "^4.13.11",
    "axios": "^1.13.4",
    "hono": "^4.11.7"
  },
  "devDependencies": {
    "@hono/vite-build": "^1.2.0",
    "@hono/vite-dev-server": "^0.18.2",
    "vite": "^6.3.5",
    "wrangler": "^4.4.0"
  },
  "keywords": [
    "ai",
    "assistant",
    "telegram",
    "bot",
    "openclaw",
    "moltbot",
    "free",
    "autonomous",
    "cloudflare",
    "hono"
  ],
  "author": "elmatador0197",
  "license": "MIT"
}
